{
  "name": "@0xshariq/timeline",
  "version": "3.0.0",
  "description": "Generate beautiful commit timeline charts for GitHub, GitLab, Bitbucket, and SourceHut repositories with an interactive CLI - supports multiple chart types",
  "main": "dist/index.js",
  "bin": {
    "timeline": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p .",
    "dev": "node --watch dist/index.js",
    "postinstall": "pnpm rebuild canvas || echo 'Canvas rebuild failed, will auto-fix on first run'"
  },
  "keywords": [
    "git",
    "timeline",
    "github",
    "gitlab",
    "bitbucket",
    "sourcehut",
    "cli",
    "visualization",
    "chart",
    "analytics",
    "commits",
    "pie-chart",
    "bar-chart",
    "heatmap",
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/0xshariq/timeline.git"
  },
  "bugs": {
    "url": "https://github.com/0xshariq/timeline/issues"
  },
  "homepage": "https://github.com/0xshariq/timeline#readme",
  "author": "0xshariq",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "@types/three": "0.181.0",
    "boxen": "^8.0.1",
    "canvas": "^3.0.0",
    "chalk": "^5.3.0",
    "chart.js": "^4.4.7",
    "chartjs-chart-matrix": "^2.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "gradient-string": "^3.0.0",
    "node-fetch": "^3.3.2",
    "ora": "^9.0.0",
    "three": "0.181.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/gradient-string": "1.1.6",
    "@types/node": "24.9.2"
  }
}
